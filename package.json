{
  "name": "nokeapi",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jorma16/nokeapi.git"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "0.18.0",
    "camelcase": "5.2.0"
  },
  "ava": {
    "files": [
      "**/*.spec.js"
    ],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "powerAssert": false
  },
  "xo": {
    "ignores": [
      "public/**"
    ],
    "space": true,
    "rules": {
      "capitalized-comments": [
        "off"
      ],
      "no-unused-vars": [
        "error",
        {
          "ignoreRestSiblings": true,
          "args": "none"
        }
      ],
      "no-warning-comments": [
        0
      ],
      "arrow-parens": [
        "error",
        "always"
      ],
      "object-shorthand": [
        "off"
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ]
    },
    "settings": {
      "import/resolver": {
        "node": {
          "paths": [
            "./"
          ]
        }
      }
    }
  },
  "devDependencies": {
    "ava": "1.3.1",
    "xo": "0.24.0"
  }
}
